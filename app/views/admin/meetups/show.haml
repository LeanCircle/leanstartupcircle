- title " | Lean Startup Circle"

#show_meetups
  %h1= @meetup.name
  = image_tag @meetup.thumbnail_url
  %p #{ @meetup.address } (#{ @meetup.latitude }, #{ @meetup.longitude })
  %p= @meetup.description.html_safe unless @meetup.description.blank?
  %p= link_to_if !@meetup.meetup_link.blank?, @meetup.meetup_link
  %p= link_to_if !@meetup.facebook_link.blank?, @meetup.facebook_link
  %p= link_to_if !@meetup.twitter_link.blank?, @meetup.twitter_link
  %p= link_to_if !@meetup.linkedin_link.blank?, @meetup.linkedin_link
  %p= link_to_if !@meetup.other_link.blank?, @meetup.other_link
  %p= link_to_if !@meetup.googleplus_link.blank?, @meetup.googleplus_link
  %p
    Approval:
    = @meetup.approval
  %p
    Circle?
    = @meetup.lsc

  = link_to 'Edit', edit_admin_meetup_path(@meetup)
  \|
  = link_to 'Destroy', admin_meetup_path(@meetup), :confirm => 'Are you sure?', :method => :delete
  \|
  = link_to 'Index', admin_meetups_path
  \|
  = link_to 'New meetup', new_admin_meetup_path
